{
  "name": "Move Pathfind Suite",
  "tests": [
    {
      "name": "Setup: mover_pathfind",
      "setup": "macro_auth_user",
      "username": "mover_pathfind",
      "password": "password"
    },
    {
      "name": "Positive: Pathfind 1 to 10",
      "command": "move.pathfind",
      "data": { "from": 1, "to": 10 },
      "user": "mover_pathfind",
      "expect": { "status": "ok" },
      "asserts": [
        { "path": "data.path.0", "op": "==", "value": 1 }
      ]
    },
    {
      "name": "Negative: Pathfind Missing Auth (Observed: ok)",
      "command": "move.pathfind",
      "data": { "from": 1, "to": 10 },
      "expect": { "status": "error", "error_code": 401  }
    }
  ]
}
