{
  "name": "S2S Blocked Commands",
  "version": "2.0.0",
  "protocol_category": "99_S2S",
  "description": "Ensures S2S commands are blocked for normal users",
  "tests": [
    {
      "name": "Setup: User",
      "setup": "macro_auth_user",
      "username": "s2s_block_user",
      "password": "password"
    },
    {
      "name": "S2S Event Relay (Blocked)",
      "command": "s2s.event_relay",
      "user": "s2s_block_user",
      "expect": { "status": "refused" }
    },
    {
      "name": "S2S Planet Genesis (Blocked)",
      "command": "s2s.planet_genesis",
      "user": "s2s_block_user",
      "expect": { "status": "refused" }
    }
  ]
}
