{
  "name": "Combat Toll Fighters Regression",
  "version": "2.0.0",
  "protocol_category": "23_Combat",
  "description": "Regression tests for toll fighter mechanics",
  "tests": [
    {
      "name": "Setup: Admin Login",
      "command": "auth.login",
      "data": { "username": "System", "passwd": "BOT" },
      "user": "admin",
      "expect": { "status": "ok" }
    },
    {
      "name": "Setup: Register Toll Collector *uuid*",
      "command": "auth.register",
      "data": { "username": "toll_collector_*uuid*", "passwd": "password123" },
      "expect": { "status": "ok" }
    },
    {
      "name": "Setup: Login Toll Collector",
      "command": "auth.login",
      "data": { "username": "toll_collector_*uuid*", "passwd": "password123" },
      "user": "toll_collector",
      "expect": { "status": "ok" },
      "save": { "toll_collector_id": "data.player_id" }
    },
    {
      "name": "Setup: Register Victim *uuid*",
      "command": "auth.register",
      "data": { "username": "toll_victim_*uuid*", "passwd": "password123" },
      "expect": { "status": "ok" }
    },
    {
      "name": "Setup: Login Victim",
      "command": "auth.login",
      "data": { "username": "toll_victim_*uuid*", "passwd": "password123" },
      "user": "toll_victim",
      "expect": { "status": "ok" },
      "save": { "victim_id": "data.player_id" }
    },
    {
      "name": "Test: Verify Toll Collector Online",
      "command": "system.time",
      "user": "toll_collector",
      "expect": { "status": "ok" }
    },
    {
      "name": "Test: Verify Victim Online",
      "command": "system.time",
      "user": "toll_victim",
      "expect": { "status": "ok" }
    },
    {
      "name": "Summary: Toll Fighter Setup Complete",
      "comment": "Created: Toll Collector (player_id @toll_collector_id) and Victim (player_id @victim_id). Ready for combat toll mechanics tests."
    }
  ]
}
