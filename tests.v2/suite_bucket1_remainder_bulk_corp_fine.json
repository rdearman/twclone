{
  "name": "Bucket 1 Remainder - Bulk, Corp, and Fine",
  "tests": [
    {
      "name": "Setup: trader_player_1",
      "setup": "macro_auth_user",
      "username": "trader_player_1",
      "password": "password"
    },
    {
      "name": "Rig: Join Corp 100",
      "command": "sys.raw_sql_exec",
      "data": { "sql": "INSERT INTO corp_members (corporation_id, player_id, role) VALUES (100, @{trader_player_1_player_id}, 'Member') ON CONFLICT DO NOTHING;" },
      "user": "admin",
      "expect": { "status": "ok" }
    },
    {
      "name": "bulk.execute - Positive",
      "command": "bulk.execute",
      "data": { "commands": [ { "command": "player.my_info", "data": {} } ] },
      "user": "trader_player_1",
      "expect": { "status": "ok" }
    },
    {
      "name": "bulk.execute - Negative (No Auth)",
      "command": "bulk.execute",
      "data": { "commands": [ { "command": "player.my_info", "data": {} } ] },
      "expect": { "status": "error", "error_code": 401 }
    },
    {
      "name": "corp.balance - Positive",
      "command": "corp.balance",
      "user": "trader_player_1",
      "expect": { "status": "ok" }
    },
    {
      "name": "corp.balance - Negative (No Auth)",
      "command": "corp.balance",
      "expect": { "status": "error", "error_code": 401 }
    },
    {
      "name": "corp.statement - Positive",
      "command": "corp.statement",
      "user": "trader_player_1",
      "expect": { "status": "ok" }
    },
    {
      "name": "corp.statement - Negative (No Auth)",
      "command": "corp.statement",
      "expect": { "status": "error", "error_code": 401 }
    },
    {
      "name": "fine.list - Positive",
      "command": "fine.list",
      "user": "trader_player_1",
      "expect": { "status": "ok" }
    },
    {
      "name": "fine.list - Negative (No Auth)",
      "command": "fine.list",
      "expect": { "status": "error", "error_code": 401 }
    }
  ]
}
