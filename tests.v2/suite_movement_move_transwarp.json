{
  "name": "Move Transwarp Suite",
  "tests": [
    {
      "name": "Setup: mover_transwarp",
      "setup": "macro_auth_user",
      "username": "mover_transwarp",
      "password": "password"
    },
    {
      "name": "Rig: Position mover_transwarp player at Sector 1",
      "command": "sys.raw_sql_exec",
      "user": "admin",
      "data": {
        "sql": "UPDATE players SET sector_id = 1 WHERE name = 'mover_transwarp';"
      },
      "expect": { "status": "ok" }
    },
    {
      "name": "Rig: Position mover_transwarp ship at Sector 1",
      "command": "sys.raw_sql_exec",
      "user": "admin",
      "data": {
        "sql": "UPDATE ships SET sector_id = 1, has_transwarp = 1 WHERE ship_id = (SELECT ship_id FROM players WHERE name = 'mover_transwarp');"
      },
      "expect": { "status": "ok" }
    },
    {
      "name": "Positive: Transwarp to Sector",
      "command": "move.transwarp",
      "data": { "to_sector_id": 10 },
      "user": "mover_transwarp",
      "expect": { "status": "ok" }
    },
    {
      "name": "Negative: Transwarp Missing Auth",
      "command": "move.transwarp",
      "data": { "to_sector_id": 10 },
      "expect": { "status": "error", "error_code": 401 }
    }
  ]
}
