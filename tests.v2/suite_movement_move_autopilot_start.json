{
  "name": "Move Autopilot Start Suite",
  "tests": [
    {
      "name": "Setup: mover_autopilot",
      "setup": "macro_auth_user",
      "username": "mover_autopilot",
      "password": "password"
    },
    {
      "name": "Positive: Start Autopilot",
      "command": "move.autopilot.start",
      "data": { "to_sector_id": 10 },
      "user": "mover_autopilot",
      "expect": { "status": "ok" }
    },
    {
      "name": "Negative: Autopilot Start Missing Auth",
      "command": "move.autopilot.start",
      "data": { "to_sector_id": 10 },
      "expect": { "status": "error", "error_code": 401 }
    }
  ]
}
