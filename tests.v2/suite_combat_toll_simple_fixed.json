{
  "name": "Combat Toll Fighters Regression",
  "version": "2.0.0",
  "protocol_category": "23_Combat",
  "description": "Regression tests for toll fighter mechanics",
  "tests": [
    {
      "name": "Setup: Admin Login",
      "command": "auth.login",
      "data": { "username": "System", "passwd": "BOT" },
      "user": "admin",
      "expect": { "status": "ok" }
    },
    {
      "name": "Setup: Create/Register Toll Collector",
      "command": "auth.register",
      "data": { "username": "toll_fighter_collector", "passwd": "password123" },
      "expect": { "status": "ok" },
      "register_if_missing": true
    },
    {
      "name": "Setup: Login Toll Collector",
      "command": "auth.login",
      "data": { "username": "toll_fighter_collector", "passwd": "password123" },
      "user": "toll_collector",
      "expect": { "status": "ok" },
      "save": { "tc_id": "data.player_id" }
    },
    {
      "name": "Setup: Create/Register Victim",
      "command": "auth.register",
      "data": { "username": "toll_fighter_victim", "passwd": "password123" },
      "expect": { "status": "ok" },
      "register_if_missing": true
    },
    {
      "name": "Setup: Login Victim",
      "command": "auth.login",
      "data": { "username": "toll_fighter_victim", "passwd": "password123" },
      "user": "toll_victim",
      "expect": { "status": "ok" },
      "save": { "tv_id": "data.player_id" }
    },
    {
      "name": "Test: Both Accounts Authenticated",
      "command": "system.time",
      "user": "toll_collector",
      "expect": { "status": "ok" }
    },
    {
      "name": "Test: Victim Authenticated",
      "command": "system.time",
      "user": "toll_victim",
      "expect": { "status": "ok" }
    }
  ]
}
