{
  "name": "Move Autopilot Stop Suite",
  "tests": [
    {
      "name": "Setup: mover_autopilot",
      "setup": "macro_auth_user",
      "username": "mover_autopilot",
      "password": "password"
    },
    {
      "name": "Positive: Stop Autopilot",
